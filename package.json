    "grape-js-emoji": "^0.0.1",
    "grape-theme": "2.x",
    "grape-web": "^0.62.3",
    "images-loader": "^0.1.2",
    "keyname": "^0.1.0",
    "lodash": "^3.10.0",
    "moment": "^2.10.3",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-finite-list": "^1.0.7",
    "react-intl": "^2.1.3",
    "react-overlays": "^0.7.0",
    "react-pure-render": "^1.0.2",
    "react-redux": "^4.0.0",
    "react-visibility-sensor": "^3.1.1",
    "redux": "^3.0.4",
    "redux-thunk": "^1.0.0",
    "reselect": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-react-intl": "^2.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^5.0.0",
    "custom-event-polyfill": "^0.2.1",
    "document-register-element": "^0.1.8",
    "eslint": "^3.14.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-config-ubergrape": "^4.1.0",
    "eslint-plugin-babel": "^2.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "expect.js": "^0.3.1",
    "font-awesome": "^4.3.0",
    "glob": "^7.0.6",
    "json-loader": "^0.5.1",
    "karma": "^0.13.8",
    "karma-chrome-launcher": "^0.1.10",
    "karma-cli": "^0.0.4",
    "karma-mocha": "^0.1.10",
    "karma-mocha-reporter": "^2.0.3",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^2.0.3",
    "lint-staged": "^3.0.1",
    "mocha": "^2.2.5",
    "node-libs-browser": "^0.5.2",
    "pre-commit": "^1.1.3",
    "raw-loader": "^0.5.1",
    "react-styleguidist": "^5.0.6",
    "reactive-elements": "^0.6.5",
    "svgo": "^0.6.1",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^1.14.0"
  },
  "scripts": {
    "all": "npm run test && npm run build && npm run i18n-export",
    "build": "npm run clear:lib && babel src --out-dir lib",
    "build:watch": "npm run clear:lib && babel --watch src --out-dir lib",
    "build:i18n": "npm run clear:i18n && npm run build && npm run i18n-export",
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "lint": "eslint ./src",
    "lint:staged": "lint-staged",
    "emoji": "bin/emoji",
    "test:watch": "cross-env NODE_ENV=test karma start",
    "test": "cross-env SINGLE_RUN=true NODE_ENV=testing karma start",
    "clear:lib": "rimraf './lib/*'",
    "clear:i18n": "rimraf './i18n/{src,export}'",
    "i18n-export": "babel-node i18n/scripts/export.js",
    "i18n-import": "npm run build && babel-node i18n/scripts/import.js",
    "prepublishOnly": "npm run all",
    "sg": "cross-env STATIC_PATH=/dist/ styleguidist server",
    "sg:build": "styleguidist build"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix --quiet",
      "git add"
    ]
  },
  "pre-commit": "lint:staged"
}
