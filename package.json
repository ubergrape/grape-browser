{
  "name": "grape-browser",
  "version": "3.6.2",
  "main": "lib/index.js",
  "engines": {
    "node": "5.11.0"
  },
  "dependencies": {
    "color": "^0.10.1",
    "component-query": "^0.0.3",
    "emojione": "^1.4.0",
    "grape-js-emoji": "^0.0.1",
    "grape-theme": "2.x",
    "grape-web": "0.x",
    "images-loader": "^0.1.2",
    "keyname": "^0.1.0",
    "lodash": "^3.10.0",
    "moment": "^2.10.3",
    "react": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-finite-list": "^1.0.4",
    "react-intl": "^2.1.3",
    "react-overlays": "^0.6.3",
    "react-pure-render": "^1.0.2",
    "react-redux": "^4.0.0",
    "react-visibility-sensor": "^3.1.1",
    "redux": "^3.0.4",
    "redux-thunk": "^1.0.0",
    "reselect": "^2.0.1"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "babel-core": "^5.6.20",
    "babel-eslint": "^4.1.5",
    "babel-loader": "^5.3.1",
    "babel-plugin-react-intl": "^1.0.1",
    "custom-event-polyfill": "^0.2.1",
    "document-register-element": "^0.1.8",
    "eslint": "^1.9.0",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-config-ubergrape": "^2.0.0",
    "eslint-plugin-babel": "^2.1.1",
    "eslint-plugin-react": "^5.0.1",
    "expect.js": "^0.3.1",
    "font-awesome": "^4.3.0",
    "glob": "^7.0.6",
    "json-loader": "^0.5.1",
    "karma": "^0.13.8",
    "karma-chrome-launcher": "^0.1.10",
    "karma-cli": "^0.0.4",
    "karma-mocha": "^0.1.10",
    "karma-mocha-reporter": "^2.0.3",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.5.1",
    "lint-staged": "^3.0.1",
    "mocha": "^2.2.5",
    "node-libs-browser": "^0.5.2",
    "pre-commit": "^1.1.3",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.2.0",
    "reactive-elements": "^0.6.5",
    "svgo": "^0.6.1",
    "svgo-loader": "^1.1.0",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "scripts": {
    "all": "npm run lint && npm run test && npm run build && npm run i18n-export",
    "build": "npm run clear:lib && babel src --out-dir lib",
    "build:watch": "npm run clear:lib && babel --watch src --out-dir lib",
    "build:i18n": "npm run clear:i18n && npm run build && npm run i18n-export",
    "start": "NODE_ENV=development webpack-dev-server --config webpack.config.development.js",
    "lint": "eslint ./src",
    "lint:staged": "lint-staged",
    "emoji": "bin/emoji",
    "test:watch": "NODE_ENV=test karma start",
    "test": "SINGLE_RUN=true NODE_ENV=testing karma start",
    "clear:lib": "rm -fr ./lib/*",
    "clear:i18n": "rm -rf ./i18n/{src,export}",
    "i18n-export": "babel-node i18n/scripts/export.js",
    "i18n-import": "npm run build && babel-node i18n/scripts/import.js",
    "prepublish": "npm run all"
  },
  "lint-staged": {
    "./src/*.js": [
      "eslint",
      "git add"
    ]
  },
  "pre-commit": "lint:staged"
}
